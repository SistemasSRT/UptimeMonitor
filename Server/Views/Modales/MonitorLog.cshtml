@{
    Layout = null;
}
<div class="modal-dialog">
    <div class="modal-header ng-scope">
        <h3 class="modal-title" id="modal-title">Logs de {{monitor.Nombre}}</h3>
    </div>
    <div class="modal-body ng-scope" id="modal-body">
        <table class="table">
            <thead>
                <tr>
                    <th class="text-center">Id</th>
                    <th class="text-center">Fecha</th>
                    <th class="text-center">TipoResultado</th>
                    <th class="text-center">Resultado</th>
                </tr>
            </thead>
            <tbody>
                <tr ng-repeat="log in logs">
                    <td class="text-right" >{{$index + (10 * pagina) + 1}}</td>
                    <td>{{log.Fecha}}</td>
                    <td class="text-center">
                        <span ng-hide="log.TipoResultado == 0" class="label label-success"> Correcto </span>
                        <span ng-hide="log.TipoResultado == 1" class="label label-danger"> Error </span>
                    </td>
                    <td>{{log.Resultado}}</td>
                </tr>
                <tr>
                    <td colspan="4" class="text-right">
                        <button class="btn btn-default" ng-disabled="pagina==0" ng-click="traerLogsAnteriores()">
                            <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
                        </button>
                        <button class="btn btn-default" ng-click="traerLogsSiguientes()">
                            <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
                        </button>
                    </td>
                </tr>
            </tbody>

        </table>
    </div>
    <div class="modal-footer ng-scope">
        <button class="btn btn-primary" type="button" ng-click="ok()">Cerrar</button>        
    </div>
</div>